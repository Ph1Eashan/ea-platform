{
  "page": { "title": "Users" },
  "data": {
    "users": {
      "type": "get",
      "url": "https://jsonplaceholder.typicode.com/users"
    }
  },
  "layout": {
    "type": "stack",
    "children": [
      {
        "type": "widget",
        "widget": "list",
        "props": {
          "items": "{{users}}",
          "render": {
            "type": "widget",
            "widget": "text",
            "props": {
              "value": "{{item.name}}"
            }
          }
        }
      }
    ]
  }
}
